{
	"name": "cart-microservices",
	"version": "0.0.1",
	"description": "Cart Microservi√ßos",
	"author": "Bruno Cardoso",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"build:services": "cd services-docker && docker-compose up --no-start && cd ..",
		"start:services": "cd services-docker && docker-compose start && cd ..",
		"stop:services": "cd services-docker && docker-compose stop && cd ..",
		"start:service:rabbitmq": "cd services-docker && docker start cart-rabbitmq-3.8.3 && cd ..",
		"stop:service:rabbitmq": "cd services-docker && docker stop cart-rabbitmq-3.8.3 && cd ..",
		"build:microservices": "nest build api; nest build cart; nest build checkout; nest build product; nest build product_price; nest build user",
		"build:ml-api": "nest build api",
		"build:ml-cart": "nest build cart",
		"build:ml-checkout": "nest build checkout",
		"build:ml-product": "nest build product",
		"build:ml-product-price": "nest build product_price",
		"build:ml-user": "nest build user",
		"build:docker-compose": "docker compose build --no-cache",
		"build:docker": "docker build --no-cache --force-rm .",
		"prisma:migrate": "npx prisma migrate dev --name cart",
		"prisma:generate": "npx prisma generate",
		"lint:format": "eslint '{lib,microservices,config,api,prisma}/**/**/**/**/*.ts' --fix; prettier --write \"lib/**/**/**/**/*.ts\" \"api/**/**/**/*.ts\" \"config/**/**/**/*.ts\" \"microservices/**/**/**/**/*.ts\" \"prisma/*.ts\"",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"start:api": "nest start api --watch",
		"start:cart": "nest start cart --watch",
		"start:checkout": "nest start checkout --watch",
		"start:user": "nest start user --watch",
		"start:product": "nest start product --watch",
		"start:product-price": "nest start product_price --watch",
		"start": "pm2 start config/ecosystem/ecosystem.microservices.config.js",
		"start:dev": "pm2 start config/ecosystem/ecosystem.microservices.config.js; pm2 logs",
		"start:docker:ml-api": "pm2-runtime start config/ecosystem/ecosystem.api.config.js",
		"start:docker:ml-cart": "pm2-runtime start config/ecosystem/ecosystem.cart.config.js",
		"start:docker:ml-checkout": "pm2-runtime start config/ecosystem/ecosystem.checkout.config.js",
		"start:docker:ml-product": "pm2-runtime start config/ecosystem/ecosystem.product.config.js",
		"start:docker:ml-product-price": "pm2-runtime start config/ecosystem/ecosystem.product-price.config.js",
		"start:docker:ml-user": "pm2-runtime start config/ecosystem/ecosystem.user.config.js"
	},
	"dependencies": {
		"@nestjs/axios": "^3.0.0",
		"@nestjs/common": "^9.0.0",
		"@nestjs/config": "^2.3.2",
		"@nestjs/core": "^9.0.0",
		"@nestjs/jwt": "^10.1.0",
		"@nestjs/microservices": "^9.4.2",
		"@nestjs/passport": "^10.0.0",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/swagger": "^6.3.0",
		"@prisma/client": "^5.0.0",
		"amqp-connection-manager": "^4.1.13",
		"amqplib": "^0.10.3",
		"axios": "^1.5.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"helmet": "^7.0.0",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.1",
		"reflect-metadata": "^0.1.13",
		"webpack": "^5.89.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.0.0",
		"@nestjs/schematics": "^9.0.0",
		"@nestjs/testing": "^9.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "28.1.8",
		"@types/node": "^16.0.0",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "28.1.3",
		"prettier": "^2.3.2",
		"prisma": "^5.0.0",
		"supertest": "^6.1.3",
		"ts-jest": "28.0.8",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.1.0",
		"typescript": "^4.7.4"
	},
	"_moduleAliases": {
		"@lib/dto/microservice/product": "lib/src/dto/microservices/product",
		"@lib/dto/microservice/product_price": "lib/src/dto/microservices/product_price",
		"@lib/dto/microservice/user": "lib/src/dto/microservices/user",
		"@lib/dto/microservice/cart": "lib/src/dto/microservices/cart",
		"@lib/dto/microservice/checkout": "lib/src/dto/microservices/checkout",
		"@lib/enum": "lib/src/enum",
		"@lib/type": "lib/src/type",
		"@lib": "lib/src",
		"@config": "config/services",
		"@api": "api/src",
		"@product": "microservices/product",
		"@product_price": "microservices/product_price",
		"@user": "microservices/user",
		"@cart": "microservices/cart",
		"@checkout": "microservices/checkout"
	},
	"_moduleDirectories": [
		"node_modules"
	],
	"prisma": {
		"schema": "prisma/schema.prisma"
	}
}
